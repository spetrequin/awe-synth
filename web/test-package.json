{
  "name": "awe-player-integration-tests",
  "version": "1.0.0",
  "description": "Comprehensive integration testing suite for AWE Player EMU8000 emulator",
  "scripts": {
    "test": "node test-runner-cli.js",
    "test:critical": "node test-runner-cli.js --suite critical",
    "test:performance": "node test-runner-cli.js --suite performance",
    "test:ci": "node test-runner-cli.js --output junit --fail-fast",
    "test:watch": "node test-runner-cli.js --watch",
    "test:browser": "npx serve . -p 3000 & open http://localhost:3000/test-comprehensive-integration.html",
    "serve": "npx serve . -p 3000"
  },
  "keywords": [
    "emu8000",
    "soundfont",
    "midi",
    "webassembly",
    "audio",
    "synthesis",
    "testing",
    "integration"
  ],
  "author": "AWE Player Team",
  "license": "MIT",
  "devDependencies": {
    "puppeteer": "^21.0.0",
    "commander": "^11.0.0",
    "chalk": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spetrequin/awe-synth.git"
  },
  "bugs": {
    "url": "https://github.com/spetrequin/awe-synth/issues"
  },
  "homepage": "https://github.com/spetrequin/awe-synth#readme"
}